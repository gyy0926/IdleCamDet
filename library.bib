@Article{loc1,
  author   = {Chen, Luan and Ahriz, Iness and Le Ruyet, Didier},
  journal  = {IEEE Internet of Things Journal},
  title    = {AoA-Aware Probabilistic Indoor Location Fingerprinting Using Channel State Information},
  year     = {2020},
  number   = {11},
  pages    = {10868--10883},
  volume   = {7},
  abstract = {随着无线通信技术的迅速发展，位置指纹技术在物联网领域催生了大量的室内基于位置的业务。
对于大多数基于模式匹配的低频解决方案，以往的工作要么是简单接收信号强度(RSS)，因为复杂的环境动力学导致性能显著下降，要么是依赖于细粒度物理层信道状态信息(CSI)，其复杂的结构导致计算复杂度增加。
同时，恶劣的室内环境也会在某些预定义参考点(RPs)之间产生相似的无线电信号，这些信号可能随机分布在感兴趣的区域，从而极大地影响定位制图的精度。
为了解决这些难题，在离线现场调查中，我们首先采用CSI振幅的自回归(AR)建模熵作为位置指纹，该方法在保留最具位置特异性的统计通道信息的同时，具有RSS结构简单的特点。
此外，通过一种增强的基于子空间的算法，可以从CSI阶段准确提取额外的到达角(AoA)指纹，进一步消除了容易出错的RP候选。
在在线阶段，利用CSI的振幅和相位信息，提出了一种新的二元核回归方法来精确推断目标的位置。
大量的室内实验结果验证了我们提出的系统优于以前的方法的定位性能。},
  doi      = {10.1109/JIOT.2020.2990314},
}

